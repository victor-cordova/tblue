{
  "name": "tman-trello-scaffolding",
  "version": "0.1.0",
  "description": "Scaffolding for trello (like yeoman but for trello)",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "test": "npm run test:typing && npm run test:lint && npm run test:unit",
    "build": "npm run prettier && babel src/ -d dist/",
    "test:unit": "jest src",
    "test:lint": "eslint src/",
    "test:typing": "flow",
    "prettier": "prettier --write src/**.js"
  },
  "keywords": [
    "Trello",
    "Yeoman"
  ],
  "author": "Victor Cordova",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-root-import": "^6.1.0",
    "eslint": "^5.12.1",
    "eslint-plugin-jest": "^22.1.3",
    "flow-bin": "^0.89.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "prettier": "1.15.3",
    "regenerator-runtime": "^0.13.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "asciiart-logo": "^0.1.16",
    "axios": "^0.18.0",
    "colors": "^1.3.3",
    "dotenv": "^6.2.0",
    "figures": "^2.0.0",
    "inquirer": "^6.2.1",
    "opn": "^5.4.0",
    "qs": "^6.6.0"
  }
}
